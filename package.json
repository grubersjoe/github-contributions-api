{
  "name": "github-contribution-api",
  "version": "1.1.3",
  "description": "Get contribution count and data for a Github user",
  "main": "src/index.js",
  "scripts": {
    "build": "rm -f build/* && tsc",
    "dev": "nodemon src/index.ts --exec 'ts-node' src/index.ts",
    "start": "npm run build && node build/index.js",
    "deploy": "npx vercel --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grubersjoe/github-contributions-api.git"
  },
  "keywords": [
    "github",
    "contributions",
    "api",
    "node"
  ],
  "author": "Sallar Kaboli <sallar.kaboli@gmail.com>",
  "contributors": [
    "Jonathan Gruber <gruberjonathan@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "git+https://github.com/grubersjoe/github-contributions-api.git"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cheerio": "^0.22.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "lodash": "^4.17.20",
    "memory-cache": "^0.2.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cheerio": "^0.22.23",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/lodash": "^4.14.166",
    "@types/memory-cache": "^0.2.1",
    "@types/node-fetch": "^2.5.7",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "vercel": "^21.0.1"
  }
